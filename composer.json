{
  "name": "jonatas-sas/yii2-m2m-behavior",
  "description": "A Yii2 behavior to easily manage many-to-many relations using ActiveRecord.",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "extension",
    "behavior",
    "many-to-many",
    "relations"
  ],
  "license": "MIT",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Jonatas Sas",
      "email": "jonatas@odara.dev.br",
      "homepage": "https://github.com/jonatas-sas"
    }
  ],
  "require": {
    "php": ">=7.4 <8.4.0",
    "yiisoft/yii2": "~2.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "yiisoft/yii2-codeception": "^2.0",
    "codeception/codeception": "^4.2"
  },
  "scripts": {
    "test": "phpunit"
  },
  "autoload": {
    "psr-4": {
      "odara\\yii\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "odara\\yii\\tests\\": "tests/"
    }
  },
  "config": {
    "process-timeout": 1800,
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    },
    "platform": {
      "php": "8.1"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
